@{
	Layout = "_CurvaDashBoardLayout";
}
<div class="dash-content">
    <div class="overview">
        <div class="title">
            <i class="uil uil-tachometer-fast-alt" style="color: #d1ab5f; background-color: transparent;"></i>
            <span class="text" style="color: gray;">Dashboard</span>
        </div>

        <div class="boxes">
            <div class="box box1">

                <i class="uil uil-eye"></i>
                <span class="text">Total users</span>
                <span class="number">50,120</span>
            </div>
            <div class="box box2">
                <i class="uil uil-shop"></i>
                <span class="text">Total Products</span>
                <span class="number">20,120</span>
            </div>
            <div class="box box3">
